{
  "name": "motc-app",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "build": "NODE_ENV=production next build",
    "dev": "next dev -p 3457",
    "lint": "next lint",
    "start": "next start",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run --config vitest.config.db.ts",
    "test:integration:watch": "vitest --config vitest.config.db.ts",
    "test:security": "vitest run tests/security --config vitest.config.ts",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "create-test-user": "tsx scripts/create-test-user.ts",
    "test:daily-email": "tsx scripts/test-daily-summary-now.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:restart": "supabase stop && supabase start",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "supabase:studio": "open http://localhost:54323",
    "supabase:migration": "supabase migration new",
    "dev:local": "npm run supabase:start && npm run dev"
  },
