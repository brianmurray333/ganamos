{
  "name": "rails",
  "dockerComposeFile": "./docker-compose.yml",
  "workspaceFolder": "/workspaces",
  "forwardPorts": [5432, 15552, 9200, 3000],
  "features": {
    "ghcr.io/devcontainers/features/docker-outside-of-docker": {}
  },
  "customizations": {
    "vscode": {
        "settings": {
          "git.autofetch": true,
          "editor.formatOnSave": true
        },
        "extensions": [
            "stakwork.staklink"
        ]
    }
  }
}